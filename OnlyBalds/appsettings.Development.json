{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Auth0Api": {
    "BaseUrl": "https://dev-ctneixyg.us.auth0.com",
    "ClientId": "{ClientId}",
    "ClientSecret": "{ClientSecret}",
    "Audience": "api://onlybalds"
  },
  "Authentication": {
    "DefaultScheme": "OpenIdConnect",
    "Schemes": {
      "OpenIdConnect": {
        "SignInScheme": "Cookies",
        "Authority": "https://dev-ctneixyg.us.auth0.com",
        "ClientId": "{ClientId}",
        "ClientSecret": "{ClientSecret}",
        "ResponseType": "code",
        "CallbackPath": "/signin-oidc",
        "SaveTokens": true,
        "Scope": [
          "openid",
          "profile",
          "Tasks.Read"
        ]
      }
    }
  },
  "TasksAPI": {
    // Do include trailing slash - see https://stackoverflow.com/a/23438417
    "BaseUrl": "https://localhost:7204/"
  }
}